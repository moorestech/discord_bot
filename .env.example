# Discord Bot Token (Required)
DISCORD_TOKEN=your_discord_bot_token_here

# Discord Application Client ID (Required)
DISCORD_CLIENT_ID=your_discord_client_id_here

# Guild ID for guild-specific command registration (Optional)
# If set, commands will be registered to this guild (faster deployment)
# If not set, commands will be registered globally
GUILD_ID=your_guild_id_here

# Port for web server (Optional, Render sets this automatically)
PORT=3000

# Node environment (Optional)
NODE_ENV=development
